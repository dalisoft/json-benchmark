{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "first_name": {
        "type": "string"
      },
      "last_name": {
        "type": "string"
      },
      "email": {
        "type": "string"
      },
      "gender": {
        "type": "string"
      },
      "age": {
        "type": "integer"
      },
      "phone": {
        "type": "string"
      },
      "country": {
        "type": "string"
      },
      "state": {
        "anyOf": [
          {
            "type": "string"
          },
          {
