          }
        ]
      },
      "city": {
        "type": "string"
      },
      "street": {
        "type": "string"
      },
      "house": {
        "type": "string"
      }
    },
    "required": [
      "id",
      "first_name",
      "last_name",
      "email",
      "gender",
      "age",
      "phone",
      "country",
      "state",
      "city",
      "street",
      "house"
    ]
